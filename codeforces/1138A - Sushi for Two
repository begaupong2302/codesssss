#include <iostream>

using namespace std;

int main()
{
    int n; cin>> n;
    int A[100001]; for (int i = 0; i < n; i++) cin>> A[i];
    int B[100000] = { };
    A[0] = 1;
    int x = 0;
    for (int i = 0; i < n; i++){
        if (A[i] != A[i+1]){
            if (x = 0){
                B[i] = i - x; 
            }
            B[i] = i 
        }
    }
}
